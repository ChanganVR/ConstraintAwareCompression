[input]
constraint_type = latency
constraint = 80
constrained_bayesian_optimization = true
network = alexnet
dataset = dtd

[cbo]
relaxation_function = one-step
fine_pruning_iterations = 1
tradeoff_factor = 1.0
exp_factor = 0.5
bo_iters = 100

[fine-tuning-dtd]
max_iter = 2000
base_lr = 0.001
momentum = 0.9
test_iters = 40
test_interval = 500
disp_interval = 100
step_iters = 100

[fine-tuning-imagenet]
max_iter = 10000
base_lr = 0.001
momentum = 0.9
test_iters = 1000
test_interval = 2000
disp_interval = 500
step_iters = 500
