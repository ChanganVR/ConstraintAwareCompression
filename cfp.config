[input]
constraint_type = latency
constraint = 70
constrained_bayesian_optimization = true
network = alexnet

[cbo]
relaxation_function = one-step
fine_pruning_iterations = 1
tradeoff_factor = 0.7
exp_factor = 0.5
bo_iters = 200

[fine-tuning]
min_acc = 0.55
max_iter = 10000
base_lr = 0.001
momentum = 0.9
gamma = 0.1
stepsize = 1000000
test_iters = 1000
test_interval = 1000
disp_interval = 500
step_iters = 500
early_stopping_iters = 10000
final_finetuning = false
